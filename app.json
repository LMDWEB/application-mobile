{
  "expo": {
    "name": "Sporty",
    "description": "Projet annuel sur le Football",
    "slug": "Sporty",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.6",
    "orientation": "portrait",
    "primaryColor": "#303a59",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/icon.png",
      "appStoreUrl": "https://itunes.apple.com/fr/app/pop-corn/id1458543106?l=fr&ls=1&mt=8",
      "supportsTablet": false,
      "isTabletOnly": false,
      "infoPlist": {
        "NSLocationAlwaysAndWhenInUseUsageDescription" : "Location is required to find nearest cinemas",
        "NSLocationUsageDescription": "Location is required to find nearest cinemas",
        "NSLocationAlwaysUsageDescription": "Location is required to find nearest cinemas",
        "NSLocationWhenInUseUsageDescription" : "Location is required to find nearest cinemas",
        "NSLocationAlwaysUsage" : "Location is required to find nearest cinemas"
      },
      "usesIcloudStorage": false
    },
    "androidStatusBarColor": "#303a59",
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#303a59"
    },
    "android": {
      "package": "com.antonbourtnik.sporty",
      "versionCode": 1,
      "icon": "./assets/icon.png",
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.antonbourtnik.popcorn",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "MANAGE_DOCUMENTS",
        "READ_EXTERNAL_STORAGE",
        "READ_PHONE_STATE",
        "VIBRATE",
        "WAKE_LOCK",
        "WRITE_EXTERNAL_STORAGE",
        "com.anddoes.launcher.permission.UPDATE_COUNT",
        "com.android.launcher.permission.INSTALL_SHORTCUT",
        "com.google.android.c2dm.permission.RECEIVE",
        "com.google.android.gms.permission.ACTIVITY_RECOGNITION",
        "com.google.android.providers.gsf.permission.READ_GSERVICES",
        "com.htc.launcher.permission.READ_SETTINGS",
        "com.htc.launcher.permission.UPDATE_SHORTCUT",
        "com.majeur.launcher.permission.UPDATE_BADGE",
        "com.sec.android.provider.badge.permission.READ",
        "com.sec.android.provider.badge.permission.WRITE",
        "com.sonyericsson.home.permission.BROADCAST_BADGE"
      ],

      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      },
      "splash": {
        "backgroundColor": "#ffffff",
        "resizeMode": "contain"
      }
    },
    "scheme": "sporty"
  }
}